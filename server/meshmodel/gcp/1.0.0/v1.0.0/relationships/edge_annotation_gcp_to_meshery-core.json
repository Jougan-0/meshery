        "styles": {
            "lineStyle": "solid",
            "lineColor": "#00B39F",
            "target-arrow-color": "#00B39F",
            "targetArrowShape": "triangle"
        },
        "capabilities": {
            "designer": {
                "edit": {
                    "style": true,
                    "config": false,
                    "label": true
                }
            }
        }
    },
    "model": {
        "schemaVersion": "core.meshery.io/v1beta1",
        "version": "v1.0.0",
        "name": "gcp",
