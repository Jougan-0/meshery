{
    "schemaVersion": "relationships.meshery.io/v1alpha3",
    "version": "v1.0.0",
    "kind": "edge",
    "type": "binding",
    "subType": "firewall",
    "selectors": [
      {
        "allow": {
          "from": [
            {
              "kind": "NATGateway",
              "model": {
                "name": "aws-ec2-controller",
                "registrant": {
                  "kind": "github"
                }
              },
              "patch": {
                "patchStrategy": "merge",
                "mutatorRef": [
                  [
                    "spec",
                    "egress"
                  ]
                ],
                "description": "The NATGateway configuration is used to define egress rules in Kubernetes services."
              }
            }
          ],
          "to": [
            {
              "kind": "Service",
              "model": {
                "name": "kubernetes",
                "registrant": {
                  "kind": "artifacthub"
                }
              },
              "patch": {
                "patchStrategy": "replace",
                "mutatedRef": [
                  [
                    "spec",
                    "externalIPs"
                  ]
                ],
                "description": "Services with external IPs may be routed through a NATGateway for outbound traffic."
              }
            }
          ]
        }
      }
    ]
  }
  